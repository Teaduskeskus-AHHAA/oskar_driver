    <limit lower="0.140" upper="1.0" effort="0.0" velocity="0.0"/>
        
  </joint>

  <joint name="oskar3_body_tilt" type="revolute">
    <parent link="base_link"/>
    <child link="oskar3_body"/>
    <origin xyz="0.215 0 0.540"/>
    <axis xyz="0 1 0"/>     
    <limit lower="0.140" upper="1.0" effort="0.0" velocity="0.0"/>
        
  </joint>
-->
</robot>